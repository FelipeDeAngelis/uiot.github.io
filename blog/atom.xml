---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Blog of Universal Internet of Things</title>
	<link href="https://uiot.github.io/atom.xml" rel="self"/>
	<link href="https://uiot.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://uiot.github.io/blog</id>
	<author>
		<name>Universal Internet of Things</name>
		<email>blog@uiot.org</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://uiot.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://uiot.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
